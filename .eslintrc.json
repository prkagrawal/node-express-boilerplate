{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/newline-after-import": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
